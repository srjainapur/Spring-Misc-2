<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <!-- Autowire by Name -->
   <bean id="empAddress1" class="com.java.autowire.byName.xml.EmployeeAddress">
   	<property name="firstName" value="Samanvi"></property>
   	<property name="lastName" value="Jainapur"></property>
   	<property name="city" value="Bangalore"></property>
   	<property name="state" value="Karanataka"></property>
   </bean>
   
   <bean id="empAddress" class="com.java.autowire.byName.xml.EmployeeAddress">
   	<property name="firstName" value="Avishka"></property>
   	<property name="lastName" value="Jainapur"></property>
   	<property name="city" value="Bangalore"></property>
   	<property name="state" value="Karanataka"></property>
   </bean>
   
   <bean id="employee" class="com.java.autowire.byName.xml.Employee" autowire="byName">
   </bean>
</beans>