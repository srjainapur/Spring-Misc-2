<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id="employee" class="com.java.spring.autowire.byname.Employee">
   	<property name="empNo" value="45506"/>
   	<property name="empName" value="Satish R Jainapur"/>
   	<property name="empEmail" value="sjainapur@zensar.com"/>
   </bean>
   
   <!-- if uncomment we get following error  :- expected single matching bean but found 2: employee,employee1 -->
   <bean id="employee1" class="com.java.spring.autowire.byname.Employee">
   	<property name="empNo" value="45507"/>
   	<property name="empName" value="Samanvi S Jainapur"/>
   	<property name="empEmail" value="samanvi@zensar.com"/>
   </bean>
   
   <bean name="empManager" class="com.java.spring.autowire.byname.EmployeeManager" autowire="byName"></bean>
   
</beans>